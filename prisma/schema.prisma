generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DATABASE_URL_UNPOOLED")
}

model Projects {
  id         String   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  title      String
  desc       String
  stack      String
  demo       String
  image      String
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt
}
